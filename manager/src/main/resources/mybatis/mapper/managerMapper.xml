<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
		PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="managerMapper">
	<select id="selectManagerList" parameterType="String" resultType="vo.AgentVo">
		select	ip		as ip
				,port	as port
		from manager
		where 1 = 1
		<if test="value != null">
			and ip = #{value}
		</if>
	</select>
</mapper>